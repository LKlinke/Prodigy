# skip
nat s;
nat die;

if(s = 0){
    s := 7;
    die := unif(1,6)
} else {
    if (s = 1){
        s := 7
        die:= unif(1,3)
    } else {
        if(s = 2){
            s := 7
            die:=unif(4,6)
        } else{
            if(s = 3){
                s := 7
                {die := 1} [2/3] { {die := 2} [1/2] {die := 3} }
            } else{
                if (s = 4){
                    s := 7
                    { die := 2 } [1/2] { die := 3 }
                } else {
                    if (s = 5){
                        s := 7
                        { die := 4 } [1/2] { die := 5 }
                    } else {
                        if (s = 6){
                            s:=7
                            { die := 6 } [2/3] { {die := 4} [1/2] {die := 5} }
                        } else {skip} // s >= 7
                    }
                }
            }
        }
    }
}