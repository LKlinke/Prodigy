# check_equality pgfexamples/template_parameter_synthesis/bit_flip_conditioning_parameter.pgcl pgfexamples/template_parameter_synthesis/invariants/bit_flip_conditioning_parameter_invariant.pgcl
bool b1
bool b2
bool b3
bool b4
nat i
nat temp

while (not ((b1 = 0) & (b2 = 0))) {
    b1 := bernoulli(1/2)
    b2 := bernoulli(1/2)
    observe( (b1 = 0 & b3 = 0) || (b1 = 1 & b3 = 1) || (b2 = 0 & b4 = 0) || (b2 = 1 & b4 = 1) )
    b3 := b1
    b4 := b2
    i := i+1
    temp := 0
}